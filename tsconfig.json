{
  "extends": "@finsweet/tsconfig",
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": "./",
    "paths": {
      "$utils/*": ["src/utils/*"]
    },
    "types": [
      "@types/google.maps",
      "@finsweet/ts-utils",
      "@types/jquery",
      "@types/mapbox-gl",
      "@types/geojson",
    ],
    "resolveJsonModule": true
  }
}
